package explicit.conversion;

public class Palindrome {

	public static void main(String[] args) {
		int a=123321,rev=0,n;
		n=a;
		while(a>0) {
			int rem=a%10;
			rev=rev*10+rem;
			a=a/10;
		}
if(rev==n) {
	System.out.println("plaindrome");	
	}
else { 
	System.out.println("Not Palindrome");
}

}
}
