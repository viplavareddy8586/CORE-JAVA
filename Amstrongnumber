package assignment.io;

public class Amstrong2 {

	public static void main(String[] args) {
		int n=153,r,sum=0;
		int t=n;
		while(n!=0) {
			r=n%10;
			n=n/10;
			sum=sum+r*r*r;
		}
		if(sum==t) {
			System.out.println("Amstrong number------>"+t);
		}else {
			System.out.println("Not an Amstrong number----->"+t);
		}

	}

}
