package explicit.conversion;

public class VideosAssignment {

	public static void main(String[] args) {
		int [] a= {19,26,3,11,20,8,1,15,6,17};
		int t;
		for(int i=0;i<a.length;i++) {
			for(int j=i+1;j<a.length;j++) {
				if(a[i]>a[j]) {
					t=a[i];
					a[i]=a[j];
					a[j]=t;
				}
			}
		}
		for(int i=0;i<a.length;i++) {
			System.out.print(a[i]+" ");
		}
		System.out.println("3rdsmallest number is "+ a[3-1]);
	}

}
