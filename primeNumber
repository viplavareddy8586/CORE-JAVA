package explicit.conversion;

public class Primenumbers {

	public static void main(String[] args) {
		int num  =17;
		int count =0;
				
		for(int i=1;i<=num;i++)
		{
			int c= num % i;// 
			if(c == 0) count=count+1;
			}
		
			
		if(count==2) System.out.println("Prime...."+num);
		else         System.out.println("Not prime..."+num);
	}

}
